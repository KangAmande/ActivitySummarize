{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome Monitor",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/chrome-monitor.js",
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ],
            "outFiles": [
                "${workspaceFolder}/out/**/*.js"
            ],
            "console": "integratedTerminal"
        }
    ]
}